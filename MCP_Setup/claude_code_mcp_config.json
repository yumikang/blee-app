{
  "mcpServers": {
    "sequential-thinking": {
      "command": "node",
      "args": [
        "/opt/homebrew/lib/node_modules/@modelcontextprotocol/server-sequential-thinking/dist/index.js"
      ]
    },
    "memory": {
      "command": "node",
      "args": [
        "/opt/homebrew/lib/node_modules/@modelcontextprotocol/server-memory/dist/index.js"
      ]
    },
    "context7": {
      "command": "node",
      "args": [
        "/opt/homebrew/lib/node_modules/@upstash/context7-mcp/dist/index.js"
      ]
    },
    "firecrawl": {
      "command": "node",
      "args": [
        "/opt/homebrew/lib/node_modules/firecrawl-mcp/dist/index.js"
      ],
      "env": {
        "FIRECRAWL_API_KEY": ""
      }
    },
    "github": {
      "command": "node",
      "args": [
        "/opt/homebrew/lib/node_modules/@modelcontextprotocol/server-github/dist/index.js"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      }
    },
    "filesystem": {
      "command": "node",
      "args": [
        "/opt/homebrew/lib/node_modules/@modelcontextprotocol/server-filesystem/dist/index.js",
        "/Users/dongeuncheon"
      ]
    },
    "google-drive": {
      "command": "node",
      "args": [
        "/opt/homebrew/lib/node_modules/@modelcontextprotocol/server-gdrive/dist/index.js"
      ],
      "env": {
        "GOOGLE_CLIENT_ID": "",
        "GOOGLE_CLIENT_SECRET": "",
        "GOOGLE_REDIRECT_URI": "http://localhost:3000/oauth/callback"
      }
    }
  }
}